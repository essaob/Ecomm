@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@{
    ViewData["Title"] = "Order";
}

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-md-6">
            <h2 class="text-primary">Order List</h2>
        </div>
     

        <table id="tblData" class="table table-bordered table-striped w-100 mt-4">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Phone Number</th>
                    <th>Email</th>
                    <th>Status</th>
                    <th>Total</th>
                </tr>
            </thead>


        </table>

    </div>
</div>
@section Scripts {
    <script type="text/javascript" src="~/js/Order.js"></script>
}